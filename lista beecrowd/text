if media >= 7.0:
    print("Aluno aprovado.")

elif media < 5.0:
    print("Aluno reprovado.")

elif media >= 5.0 and media <= 6.9:
    print("Aluno em exame")
    nota = float(input())
    print("Nota do exame {:.f}".format(nota))
    media2= (media + nota) / 2

    if media2 > 5.0:
        print("Aluno aprovado.")
        media3 = (n1 + n2 + n3 + n4 + nota) // 5
        print("Media final: {:.1f}".format(media3))

    else:
        print("Aluno reprovado.")